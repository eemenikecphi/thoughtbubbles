{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/app.html"
    }
  ],
  "headers": [
    {
      "source": "/app.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/app.html",
      "headers": [
        {
          "key": "Content-Type", 
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}